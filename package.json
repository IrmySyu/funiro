{
  "name": "fls_start_gulp",
  "version": "1.0.0",
  "description": "Start template with Gulp",
  "author": "FLS",
  "private": true,
  "license": "GNU General Public License (GPL)",
  "scripts": {
    "dev": "gulp && webpack serve --progress -c config/webpack.dev.js",
    "build": "gulp build --build",
    "sprite": "gulp sprite",
    "fonts": "gulp fonts --rewrite",
    "deploy": "gulp deployFTP --build",
    "zip": "gulp deployZIP --build",
    "devbuild": "gulp build --build --nowebp",
    "tmp": "gulp -f gulpfile.tmp.js",
    "bem": "fls-bemtree-from-html"
  },
  "devDependencies": {
    "browser-sync": "latest",
    "del": "latest",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "latest",
    "gulp-clean-css": "latest",
    "gulp-file-include": "latest",
    "gulp-fonter": "latest",
    "gulp-group-css-media-queries": "latest",
    "gulp-imagemin": "7.1.0",
    "gulp-newer": "latest",
    "gulp-plumber": "latest",
    "gulp-rename": "latest",
    "gulp-sass": "latest",
    "gulp-ttf2woff": "latest",
    "gulp-ttf2woff2": "latest",
    "gulp-uglify-es": "latest",
    "gulp-webp-html": "latest",
    "gulp-webpcss": "latest",
    "imagemin-webp": "6.0.0",
    "sass": "latest",
    "transform-runtime": "0.0.0",
    "webp-converter": "2.2.3"
  }
}
